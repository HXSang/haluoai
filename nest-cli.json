{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      {
        "include": "../node_modules/.prisma/client/**/*",
        "outDir": "dist/node_modules/.prisma/client",
        "watchAssets": true
      }
    ]
  }
}
